               <children>
                  <Label alignment="CENTER" style="&#10;" text="Address 2*" textAlignment="CENTER" wrapText="false">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                      <Label alignment="CENTER" style="&#10;" text="*Not Required" textAlignment="CENTER" wrapText="false">
                          <font>
                              <Font size="10.0" />
                          </font>
                      </Label>
               </children>
            </VBox>
            </children>
        </GridPane>
    </children>
</AnchorPane>
