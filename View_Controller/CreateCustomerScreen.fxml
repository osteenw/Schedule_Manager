<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="419.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.CreateCustomerController">

    <children>
        <Label fx:id="titleLabel" alignment="CENTER" layoutX="14.0" layoutY="14.0" style="&#10;" text="Create Customer" textAlignment="CENTER" wrapText="false">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <HBox layoutX="311.0" layoutY="380.0" spacing="5.0">
            <children>
                <Button fx:id="saveButton" mnemonicParsing="false" onAction="#save" prefHeight="25.0" prefWidth="60.0" text="Save" />
                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="60.0" text="Cancel" />
            </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
        </HBox>
        <GridPane layoutX="54.0" layoutY="54.0" prefHeight="312.0" prefWidth="354.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="110.0" minWidth="10.0" prefWidth="108.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="205.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label alignment="CENTER" style="&#10;" text="Name" textAlignment="CENTER" wrapText="false">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <Label alignment="CENTER" style="&#10;" text="Address" textAlignment="CENTER" wrapText="false" GridPane.rowIndex="1">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <TextField fx:id="nameField" GridPane.columnIndex="1" />
                <TextField fx:id="addressField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <TextField fx:id="address2Field" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label alignment="CENTER" style="&#10;" text="City" textAlignment="CENTER" wrapText="false" GridPane.rowIndex="3">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label alignment="CENTER" style="&#10;" text="Postal Code" textAlignment="CENTER" wrapText="false" GridPane.rowIndex="4">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label alignment="CENTER" style="&#10;" text="Country" textAlignment="CENTER" wrapText="false" GridPane.rowIndex="5">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label alignment="CENTER" style="&#10;" text="Phone Number" textAlignment="CENTER" wrapText="false" GridPane.rowIndex="6">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="cityField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="postalcodeField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="countryField" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField fx:id="phoneField" GridPane.columnIndex="1" GridPane.rowIndex="6" />
